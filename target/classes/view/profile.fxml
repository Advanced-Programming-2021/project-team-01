<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="main" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProfileView">

    <stylesheets>
        <URL value="@profile.css" />
    </stylesheets>
   <children>
      <Button layoutX="462.0" layoutY="89.0" mnemonicParsing="false" onMouseClicked="#changeNickname" prefHeight="41.0" prefWidth="123.0" text="Change Nickname">
         <font>
            <Font name="Impact" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Bloom threshold="0.74" />
         </effect>
      </Button>
      <Button layoutX="462.0" layoutY="179.0" mnemonicParsing="false" onMouseClicked="#changePassword" prefHeight="41.0" prefWidth="123.0" text="Change Password">
         <font>
            <Font name="Impact" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Bloom threshold="0.53" />
         </effect>
      </Button>
      <Button layoutX="462.0" layoutY="266.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="35.0" prefWidth="123.0" text="back">
         <font>
            <Font name="Impact" size="17.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Bloom threshold="0.5" />
         </effect>
      </Button>
      <VBox alignment="CENTER" layoutX="31.0" layoutY="28.0" prefHeight="195.0" prefWidth="199.0">
         <children>
            <ImageView fx:id="profileImage" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
            <Region prefHeight="19.0" prefWidth="199.0" />
            <HBox alignment="CENTER" prefHeight="27.0" prefWidth="199.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username : " />
                  <Text fx:id="username" strokeType="OUTSIDE" strokeWidth="0.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="27.0" prefWidth="199.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nickname : " />
                  <Text fx:id="nickname" strokeType="OUTSIDE" strokeWidth="0.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>

</AnchorPane>
