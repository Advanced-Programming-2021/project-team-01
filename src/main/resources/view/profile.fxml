<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="main" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProfileView">

    <stylesheets>
        <URL value="@profile.css" />
    </stylesheets>
   <children>
      <Button onMouseClicked="#changeNickname" layoutX="72.0" layoutY="96.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="123.0" text="Change Nickname">
         <font>
            <Font name="Impact" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Bloom threshold="0.74" />
         </effect>
      </Button>
      <Button onMouseClicked="#changePassword" layoutX="72.0" layoutY="175.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="123.0" text="Change Password">
         <font>
            <Font name="Impact" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Bloom threshold="0.53" />
         </effect>
      </Button>
      <Button onMouseClicked="#back" layoutX="72.0" layoutY="265.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="123.0" text="back">
         <font>
            <Font name="Impact" size="17.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Bloom threshold="0.5" />
         </effect>
      </Button>
   </children>

</AnchorPane>
